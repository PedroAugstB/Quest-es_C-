#include <iostream>
#include <string>
using namespace std;

int main () {

    int  hora_inicio {0}, minuto_inicio {0};
    int  hora_fim {0}, minuto_fim {0};
    

    long minutos_totais_inicio {0}, minutos_totais_fim {0}, minutos_total{0};

    int duração_hora, duração_minuto;
    
    cin >> hora_inicio >> minuto_inicio >> hora_fim >> minuto_fim;
    
    if ( hora_inicio == hora_fim && minuto_inicio == minuto_fim ) { 
        cout <<  "O JOGO DUROU 24 HORA(S) E 0 MINUTO(S)" << endl;
        return 0;}
    
    minutos_totais_inicio  = ( hora_inicio * 60 ) + ( minuto_inicio );                     
    minutos_totais_fim     = ( hora_fim * 60 ) + ( minuto_fim );
    
    if ( minutos_totais_fim <= minutos_totais_inicio ) {
        minutos_totais_fim  += 24 * 60;
    };
    
    minutos_total          = ( minutos_totais_fim - minutos_totais_inicio );
    
    duração_hora = minutos_total / ( 60 );
    minutos_total = minutos_total % ( 60 );
    duração_minuto = minutos_total;
    
    cout << "O JOGO DUROU " << duração_hora << " HORA(S) E " << duração_minuto << " MINUTO(S)"<< endl;

}